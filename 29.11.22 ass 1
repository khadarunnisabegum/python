a=int(input("enter a value="))
b=int(input("enter b value="))
c=int(input("enter c value="))
d=int(input("enter d value="))
if a>b:
    if a>c:
        if a>d:
            print("a is big")
        else:
            print("d is big")
    else:
        if c>d:
            print("c is big")
        else:
            print("d is big")
else:
    if b>c:
        if b>d:
           print("b is big")
        else:
            print("d is big")
    else:
       if c>d:
          print("c is big")
       else:
          print("d is big")
